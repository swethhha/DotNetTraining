@model HostelManagement.MVC.Models.RoomViewModel

<h2>Room Details</h2>
<p><strong>Room Number:</strong> @Model.RoomNumber</p>
<p><strong>Capacity:</strong> @Model.Capacity</p>
<p><strong>Current Occupancy:</strong> @Model.CurrentOccupancy</p>

<h3>Students</h3>
<ul>
    @foreach (var student in Model.Students)
    {
        <li>@student</li>
    }
</ul>

<a asp-controller="RoomView" asp-action="Index">Back to List</a>
